{
  "name": "pdf-to-csv",
  "version": "1.0.0",
  "description": "Cli base script that convert pdf to csv",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node ./src/index.ts",
    "ignore": [
      "dist",
      "node_modules"
    ],
    "delay": "250",
    "env": {
      "NODE_ENV": "development"
    },
    "restartable": "rs",
    "events": {
      "start": "echo 'Starting application...'",
      "restart": "echo 'Restarting application...'"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "pdf-parse": "^2.4.5",
    "zod": "^4.3.5"
  }
}
